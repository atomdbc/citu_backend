{
    "version": 2,
    "env": {
        "GOOGLE_CREDENTIALS": "@google_credentials"
    },
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node",
            "config": { "includeFiles": ["dist/**"] }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ]
}